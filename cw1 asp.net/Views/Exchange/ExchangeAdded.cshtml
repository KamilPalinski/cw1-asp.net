@model AddConfirmationModel



<h1>Added records</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Name
            </th>
            <th>
                Description
            </th>
            <th>
                IsVisible
            </th>
        </tr>
    </thead>
    <tbody>

        
        @foreach (var item in ViewBag.Items)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.Description
                </td>
                <td>
                    @item.IsVisible
                </td>
            </tr>
}
    </tbody>



</table>